{
  "name": "my-app",
  "version": "1.0.0",
  "description": "Express + TypeScript + Prisma + Postgres starter",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --legacy-watch src/server.ts | npx pino-pretty",
    "start": "node dist/src/server.js | npx pino-pretty",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc -p .",
    "prettier:fix": "prettier --write src/**/*.ts",
    "prepare": "husky",
    "test": "dotenv -e .env.test -- jest --runInBand --detectOpenHandles",
    "test:watch": "dotenv -e .env.test -- jest --watch"
  },
  "keywords": [
    "typescript",
    "express",
    "prisma",
    "postgres",
    "pino",
    "eslint",
    "prettier",
    "nodemon"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@prisma/client": "^6.16.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "pg": "^8.16.3",
    "pino": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^30.1.3",
    "nodemon": "^3.1.0",
    "onchange": "^7.1.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
